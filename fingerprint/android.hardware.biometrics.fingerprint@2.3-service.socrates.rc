service vendor.fps_hal_socrates /odm/bin/hw/android.hardware.biometrics.fingerprint@2.3-service.socrates
    class late_start
    user system
    group system input uhid

service mfp-daemon /vendor/bin/hw/mfp-daemon
    user system
    group system drmrpc diag input uhid
    override

on property:init.svc.vendor.fps_hal_socrates=running
    start mfp-daemon

on boot
    write /sys/devices/platform/goodix_ts.0/fod_gesture 1
